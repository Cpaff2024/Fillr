# This line tells CocoaPods your app is for iOS, version 15.0 or higher.
# Adjust '15.0' if your app targets an older version.
platform :ios, '15.0'

# This tells CocoaPods which project to add the libraries to.
target 'WaterRefill2.0' do
  # This line is important for Swift projects.
  use_frameworks!

  # This is the line that adds Google Maps to your project.
  pod 'GoogleMaps'

end
